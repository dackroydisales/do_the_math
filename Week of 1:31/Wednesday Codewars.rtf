function tribonacci(signature,n)\{\
  let my_ary = [
\f2\i ...
\f1\i0 signature]\
  if(n <= 3)\{return my_ary.slice(0, n);\}\
  \
  for(let i = 3; i < n; i++)\
  \{\
    my_ary.push(my_ary[i - 1] + my_ary[i - 2] + my_ary[i - 3]);\
  \}\
  return my_ary;\
\}\
