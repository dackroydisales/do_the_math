function findOdd(A) \{\
  let tally = \{\}\
  for(let e = 0; e < A.length; e++)\
  \{\
    if(tally[A[e]])\{tally[A[e]] += 1;\}\
    else\{tally[A[e]] = 1;\}\
  \}\
  for(const ele in tally)\
  \{\
    if(tally[ele] % 2 !== 0)\{return Number(ele);\}\
  \}\
  return 0;\
\}\
